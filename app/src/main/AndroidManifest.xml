<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qianfeng.mingyihui" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".other.ui.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".other.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 引导页 -->
        <activity android:name=".other.ui.GuideActivity" >
        </activity>
        <activity android:name=".other.ui.HomeActivity" >
        </activity>

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />

        <activity android:name=".texiu.ui.TeXiu_WenZhenActivity" >
        </activity>
        <activity android:name=".texiu.ui.TeXiu_PeiZhenActivity" >
        </activity>
        <activity android:name=".texiu.ui.TeXiu_ChuZhenActivity" >
        </activity>
        <activity android:name=".texiu.ui.TeXiu_YiZhenActivity" >
        </activity>
        <activity android:name=".mingyibang.ui.LoginActivity" >
        </activity>
        <activity android:name=".yiliaoquan.ui.FuErActivity" >
        </activity>
        <activity android:name=".mingyibang.ui.MingYiBangActivity" >
        </activity>
    </application>

</manifest>
